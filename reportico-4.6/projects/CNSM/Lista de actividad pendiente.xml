<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Lista de actividad pendiente OLD</ReportTitle><ReportDescription></ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont>kozgopromedium</pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
id_operacion,
nombre_barco,
nombre_mov,
nombre,
apellido,
id_zona,
nombre,
apellido,
rey_barco,
fecha_operacion,
coment_operacion,
nom_zona
FROM tabla_barcos
LEFT JOIN `cnsm`.`tabla_operaciones` ON `tabla_barcos`.`id_barco` = `tabla_operaciones`.`id_barco` 
LEFT JOIN `cnsm`.`indice_movimiento` ON `tabla_operaciones`.`id_movimiento` = `indice_movimiento`.`id_mov` 
LEFT JOIN `cnsm`.`usuarios` ON `tabla_operaciones`.`id_usuario` = `usuarios`.`id_usuario` 
LEFT JOIN `cnsm`.`nom_zona` ON `tabla_operaciones`.`zona` = `nom_zona`.`id_zona` 
where `tabla_operaciones`.`realizado`!='si'
AND (id_movimiento ='5'  OR id_movimiento ='8' OR id_movimiento ='9' OR id_movimiento ='10' )
ORDER BY `tabla_operaciones`.`fecha_operacion` ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>id_operacion</Name><TableName></TableName><ColumnName>id_operacion</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>nombre_barco</Name><TableName></TableName><ColumnName>nombre_barco</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>nombre_mov</Name><TableName></TableName><ColumnName>nombre_mov</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>nombre</Name><TableName></TableName><ColumnName>nomb